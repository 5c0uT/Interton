adding std.io

using mermaid.struct;
using mermaid.cluster(1);

adding <py> py_seed(n: int) -> int {
    return n * 2
}

adding <c> c_mix(x: int) -> int {
    return x + 5;
}

adding <cpp> cpp_finish(x: int) -> int {
    return x * 3;
}

call main() -> int {
    int base = py_seed(3)
    int mid = c_mix(base)
    int out = cpp_finish(mid)
    inscribe("py->c->cpp:", out)
    return 0
}
