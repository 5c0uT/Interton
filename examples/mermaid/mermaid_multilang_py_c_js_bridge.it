adding std.io

using mermaid.struct;
using mermaid.cluster(1);

adding <py> py_seed(n: int) -> int {
    return n * 3
}

adding <c> c_mix(x: int) -> int {
    return x + 9;
}

adding <js> js_label(x: int) -> string {
    return "js:" + x;
}

call main() -> int {
    int base = py_seed(2)
    int mid = c_mix(base)
    string out = js_label(mid)
    inscribe("py->c->js:", out)
    return 0
}
