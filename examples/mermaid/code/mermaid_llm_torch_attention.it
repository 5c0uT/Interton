adding std.io

using mermaid.struct;
using mermaid.cluster(1);
using mermaid.deep(attn_rules);

adding <py> llm_attention_score(ids: int[]) -> float {
    import torch
    torch.manual_seed(7)
    if len(ids) == 0:
        return 0.0
    dim = 4
    emb = torch.randn(32, dim)
    x = emb[torch.tensor(ids)]
    Wq = torch.randn(dim, dim)
    Wk = torch.randn(dim, dim)
    Wv = torch.randn(dim, dim)
    q = x[0].unsqueeze(0) @ Wq
    k = x @ Wk
    v = x @ Wv
    scores = (q @ k.T) / (dim ** 0.5)
    weights = torch.softmax(scores, dim=-1)
    out = weights @ v
    return float(out.sum().item())
}

call main() -> int {
    int[] ids = [1, 3, 5, 7]
    float score = llm_attention_score(ids)
    inscribe("attention_score:", score)
    return 0
}

