adding std.io

call main() -> int {
    int[] values = [10, 20, 30, 40, 50]
    inscribe("len:", |values|)
    inscribe("slice 1..3:", values[1..3].to_string())
    values[2] = 77
    inscribe("updated:", values.to_string())

    (int, string, bool) item = (7, "seven", true)
    inscribe("tuple:", item.0, item.1, item.2)

    type Point = (x: float, y: float)
    Point p = (2.25, 3.75)
    inscribe("point:", p.x, p.y)

    inscribe("last:", values[-1].to_string())
    return 0
}
