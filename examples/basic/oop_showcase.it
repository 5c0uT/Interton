adding std.io

class Counter {
    public call inc(value: int) -> int {
        return value + 1
    }

    public call add(a: int, b: int) -> int {
        return a + b
    }
}

call main() -> int {
    Counter counter = new Counter()
    inscribe("counter inc:", counter.inc(4))
    inscribe("counter add:", counter.add(3, 7))
    return 0
}
